{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "name": "article",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext"
    },
    "date": {
      "type": "date",
      "required": true
    },
    "annexes": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "authors": {
      "via": "articles",
      "collection": "membre"
    },
    "category": {
      "model": "category",
      "via": "articles"
    },
    "mainPhoto": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "bestOf": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "preview": {
      "type": "string",
      "required": true
    }
  }
}
